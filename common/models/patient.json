{
  "name": "patient",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string"
    },
    "current_status": {
      "type": "string"
    },
    "check_in_time": {
      "type": "date"
    },
    "no_show_ups": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "appointment": {
      "type": "hasMany",
      "model": "appointment",
      "foreignKey": "pat_id"
    },
    "scheduledAppointments": {
      "type": "hasMany",
      "model": "scheduled_appointment",
      "foreignKey": "pat_id"
    },
    "openlists": {
      "type": "hasMany",
      "model": "openlist",
      "foreignKey": "pat_id"
    }
  },
  "acls": [],
  "methods": {}
}
